<Window x:Name="Settings" x:Class="WebTimer.ConfigEditor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WebTimer Settings" Height="480" Width="510"
        ResizeMode="NoResize">
    <DockPanel>
        <Frame x:Name="MainFrame">
            <Frame.Template>
                <ControlTemplate TargetType="Frame">
                    <DockPanel Margin="0">
                        <StackPanel Visibility="Hidden"
                        Margin="0"
                        Orientation="Horizontal"
                        DockPanel.Dock="Top">
                        </StackPanel>

                        <Border>
                            <ContentPresenter />
                        </Border>
                    </DockPanel>
                </ControlTemplate>
            </Frame.Template>
        </Frame>
    </DockPanel>
</Window>
